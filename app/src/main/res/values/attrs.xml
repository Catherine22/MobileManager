<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--Refer to android-sdk-macosx/platforms/android-23/data/res/values/attrs.xml-->
    <declare-styleable name="TextView">
        <attr name="title_" format="string" />
        <attr name="desc_on" format="string" />
        <attr name="desc_off" format="string" />
    </declare-styleable>

    <declare-styleable name="Banner">
        <attr name="show_background" format="boolean" />
        <attr name="show_dots" format="boolean" />
        <attr name="dots_src_on" format="integer" />
        <attr name="dots_src_off" format="integer" />
    </declare-styleable>


    <declare-styleable name="SwipeListView">
        <attr name="background_color" format="integer" />
        <attr name="swipe_mode" format="string" />
    </declare-styleable>
</resources>